package com.firstMaven.Selenium;

import org.junit.jupiter.api.*;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class AppTest {

    WebDriver driver;

    @BeforeEach
    public void setup() {
        driver = new ChromeDriver();
        driver.manage().window().maximize();
    }

    @Test
    public void login() {
        driver.get("https://onlinefeestechnocrats.in/LoginStudent.aspx?type=F");
        Assertions.assertTrue(driver.getTitle().contains("Login"));
    }

    @Test
    public void navigate() {
        driver.get("https://technocratsgroup.edu.in/");
        Assertions.assertTrue(driver.getTitle().length() > 0);
    }

    @AfterEach
    public void tearDown() {
        driver.quit();
    }
}
